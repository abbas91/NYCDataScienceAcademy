mtx_multiply <- function(mtx, vct) {
vct_rlt <- numeric(nrow(mtx))
n_col <- ncol(mtx)
n_row <- nrow(mtx)
for(i in 1:n_row) {
s <- 0
for(j in 1:n_col) {
s <- s + mtx[i, j] * vct[j]
}
vct_rlt[i] <- s
}
return(vct_rlt)
}
mtx_multiply(mtx, vct)
